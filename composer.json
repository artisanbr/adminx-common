{
    "name": "artisanbr/adminx-common",
    "version": "dev-stable",
    "description": "Adminx Common Assets",
    "keywords": [
        "laravel",
        "site builder",
        "site admin",
        "dashboard"
    ],
    "homepage": "https://artisan.dev.br",
    "license": "MIT",
    "authors": [
        {
            "name": "Renalcio Carlos Jr.",
            "homepage": "https://github.com/renalcio",
            "role": "CEO"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "canonical": true,
            "url": "../../*/*",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/artisanbr/laravel-bs-components.git",
            "reference": "master"
        }
    ],
    "require": {
        "laravel/framework": ">=10",
        "artisan-labs/laravel-breadcrumbs": "*",
        "artisan-labs/laravel-geo-database": "*",
        "artisan-labs/laravel-gmodel": "*",
        "artisan-labs/laravel-router": "*",
        "artisan-labs/laravel-ide-helper-plus": "*",
        "artisan-labs/laravel-bs-components": "*",
        "delight-im/random": "^1.0",
        "league/flysystem-ftp": "^3.0",
        "butschster/meta-tags": "^2.2",
        "spatie/laravel-menu": "^4.1",
        "google/recaptcha": "^1.2",
        "twig/twig": "^3.5",
        "scssphp/scssphp": "^1.11",
        "prologue/alerts": "^1.0",
        "genealabs/laravel-model-caching": "^0.13.2",
        "spatie/laravel-permission": "^5.8",
        "voku/html-min": "^4.5",
        "spatie/laravel-tags": "^4.3",
        "tucker-eric/eloquentfilter": "^3.0",
        "creativeorange/gravatar": "^1.0",
        "arcanedev/log-viewer": "^10.0",
        "cybercog/laravel-ban": "^4.7",
        "spatie/laravel-html": "^3.2",
        "questocat/laravel-referral": "^1.0",
        "spatie/laravel-activitylog": "^4.5",
        "yadahan/laravel-authentication-log": "^1.4",
        "laravelcollective/html": "*"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "barryvdh/laravel-debugbar": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Adminx\\Common\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adminx\\Common\\AdminxCommonServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
